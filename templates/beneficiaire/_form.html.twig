{% extends './base_front.html.twig' %}
{% block body %}

	<section class="account-wrap ptb-100">

		<div class="container">
			<div class="account-form">
        
				{{ form_start(form) }}

				<div>
					<div class="section-title style1 text-center mb-40">
						<span>Beneficier</span>
						<h2>Informations Personnelles
						</h2>
					</div>
					<div class="row">
						<div class="col-md-612">
							<div class="form-group">


								<div class="row">
									<div class="col-md-6">
										<div class="form-group">
											<label for="rib_benef">Rib du beneficiaire</label>
											{{ form_widget(form.rib_benef,{'attr':{'class':'form-control','placeholder':"rib_benef"}})}}
											{{ form_errors(form.rib_benef) }}
										</div>
									</div>
                                    <div class="col-md-6">
										<div class="form-group">
											<label for="nom_benef">Nom de beneficiaire</label>
											{{ form_widget(form.nom_benef,{'attr':{'class':'form-control','placeholder':"nom_benef"}})}}
											{{ form_errors(form.nom_benef) }}
										</div>
									</div>
									<div class="col-md-6">
										<div class="form-group">
											<label for="prenom_benef">Prenom de beneficiaire</label>
											{{ form_widget(form.prenom_benef,{'attr':{'class':'form-control','placeholder':"prenom_benef"}})}}
											{{ form_errors(form.prenom_benef) }}
										</div>
									</div>
									<div class="col-md-6">
										<div class="form-group">
											<label for="email_benef">Email de beneficiaire</label>
											{{ form_widget(form.email_benef,{'attr':{'class':'form-control','placeholder':"email_benef"}})}}
											{{ form_errors(form.email_benef) }}
										</div>
									</div>
						</div>


						<br/>

					</div>
				</div>

				
                  <button type="submit" class="btn btn-primary">Save</button>
                  
				{{ form_end(form) }}
			</div>
		</div>


	</section>


{% endblock %}
